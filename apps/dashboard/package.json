{
  "name": "@furlow/dashboard",
  "version": "1.0.1",
  "description": "Web dashboard for FURLOW Discord bots",
  "keywords": ["furlow", "discord", "dashboard", "web"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/virgilvox/discord-furlow",
    "directory": "apps/dashboard"
  },
  "author": "virgilvox",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:server && pnpm run build:client",
    "build:client": "vite build",
    "build:server": "tsup",
    "clean": "rm -rf dist .turbo",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsup --watch",
    "lint": "eslint src/ server/",
    "start": "node dist/server/index.js",
    "test": "vitest run --passWithNoTests",
    "test:unit": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@furlow/core": "workspace:*",
    "@furlow/discord": "workspace:*",
    "@furlow/schema": "workspace:*",
    "@furlow/storage": "workspace:*",
    "express": "^4.18.0",
    "express-session": "^1.18.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "prom-client": "^15.1.0",
    "ws": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.11.0",
    "@types/passport": "^1.0.16",
    "@types/ws": "^8.5.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  }
}
